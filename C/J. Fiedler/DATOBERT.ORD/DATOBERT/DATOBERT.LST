Procedure Datendruck
  Lprint Chr$(27);Chr$(112);Chr$(1)
  Lprint Chr$(27);Chr$(45);Chr$(1)
  Lprint Ueb1$
  Lprint Chr$(27);Chr$(45);Chr$(0);
  Lprint Ueb2$
  Lprint Chr$(27);Chr$(112);Chr$(0)
  Lprint Chr$(15)
  If Spa<8
    Ix=1
    Spax=Spa
    Gosub Druckdat
  Else
    Ix=1
    Spax=7
    Gosub Druckdat
    Lprint
    Ix=8
    Spax=Spa
    Gosub Druckdat
  Endif
Return
