Procedure Eingabe
  Aen=0
  If Leiste$(Menu(0))=" Ñndern"
    Aen=1
  Endif
  Cls
  Deftext 1,0,0,13
  Deffill 1,2,2
  Pbox 20,20,618,300
  Print "   *graphdat*     EINGABE :"
  If Aen=1
    Print At(1,1);"   *graphdat*     énderung :"
    Print At(5,20);"Falls ein Wert geÑndert werden soll, geben Sie bitte jeweils den"
    Print At(5,21);"neuen Wert ein. Sonst weiter mit 'RETURN'"
    Print At(5,22);"               ** press any key **"
    X=Inp(2)
  Endif
  Gosub Loeschen
  Print At(5,21);"Bitte geben Sie die HauptÅberschrift ein (<=35 Zeichen) !"
  Form Input 35,A$
  If A$<>""
    Ueb1$=A$
  Endif
  Deftext 1,0,0,32
  Text 30,65,Ueb1$
  Gosub Loeschen
  Print At(5,21);"Bitte geben Sie den Untertitel ein (<=70 Zeichen) !"
  Form Input 70,A$
  If A$<>""
    Ueb2$=A$
  Endif
  Deftext 1,0,0,6
  Text 30,83,Ueb2$
  Gosub Loeschen
  Ra=0
  If Aen=1
    Print At(5,21);"Soll eine ganze Reihe entfernt werden (j/n) ?";
    Input An$
    If An$="j"
      Ra=1
      Gosub Loeschen
      Print At(5,21);"welche Reihe soll gelîscht werden (Nr.: )?";
      Input Nr
      If Nr<Rei
        For I=Nr To Rei-1
          N$(I)=N$(I+1)
          For J=1 To Spa
            Swap W(I,J),W(I+1,J)
          Next J
        Next I
      Endif
      Rei=Rei-1
    Endif
  Endif
  Gosub Loeschen
  If Ra=0
    Print At(5,21);"Wieviele Reihen (hintereinander  (<=4))";
    Input A$
    If A$<>""
      Rei=Val(A$)
      If Rei>4
        Rei=4
      Endif
    Endif
  Endif
  Print At(5,21);"Wieviele Spalten (nebeneinander) (<=14 )  ";
  Input A$
  If A$<>""
    Spa=Val(A$)
  Endif
  Text 30,130,Rei
  Text 50,130,"Reihe(n)"
  Text 30,150,Spa
  Text 50,150,"Spalten"
  Gosub Loeschen
  For I=1 To Rei
    Deftext 1,0,0,13
    Print At(5,21);"Bezeichnung der Reihe ";I,
    Form Input 14,A$
    If A$<>""
      N$(I)=A$
    Endif
    Deftext 1,0,0,6
    Text 35,155+20*I,N$(I)
    Gosub Loeschen
  Next I
  W$=""
  If Aen=1
    Print At(5,21);"Sollen Zahlenwerte geÑndert werden (Daten) (n/j)?";
    Input W$
    Gosub Loeschen
  Endif
  If W$<>"n"
    For I=1 To Rei
      Deftext 1,0,0,13
      Print At(5,21);I;". Reihe :"
      Deftext 1,0,0,6
      Text 35,145+20*I,I
      Text 45,145+20*I,". Reihe:"
      For J=1 To Spa
        Deftext 1,0,0,13
        Print At(5,22);"                            "
        Print At(5,22);"Wert#";J;" (<10000)";
        Input A$
        If A$<>""
          W(I,J)=Val(A$)
        Endif
        Deftext 1,0,0,6
        Text 100+36*J,155+20*I,W(I,J)
      Next J
      Gosub Loeschen
    Next I
  Endif
  Deftext 1,0,0,13
  Print At(5,21);"Eingabe des maximalen y-Anzeigewerts (textmaximum):"
  Print At(5,22);"(falls mit dem hîchsten Zahlenwert identisch --> RETURN)";
  Input A$
  A=Val(A$)
  If A<>0
    Maks=A
  Endif
  Gosub Loeschen
  W$=""
  If Aen=1
    Print At(5,21);"Sollen Spaltenbezeichnungen geÑndert werden (Daten) (n/j)?";
    Input W$
    Gosub Loeschen
  Endif
  If W$<>"n"
    For I=1 To Spa
      Print At(5,21);"1.Bezeichnung der Spalte ";I
      Form Input Int(50/Spa+1),A$
      If A$<>""
        Nsa$(I)=A$
      Endif
      Deftext 1,0,0,6
      Text 30+(I-1)*500/Spa,260,Nsa$(I)
      Gosub Loeschen
      Print At(5,21);"2.Bezeichnung der Spalte ";I
      Form Input Int(70/Spa+1),A$
      If A$<>""
        Nsb$(I)=A$
      Endif
      Deftext 1,0,0,4
      Text 30+(I-1)*500/Spa,280,Nsb$(I)
      Gosub Loeschen
    Next I
  Endif
  Gosub Loeschen
  Print At(5,21);"ENDE DER EINGABE"
  Mnx=3
  Gosub Mnakt
Return
Cls
Do
  On Menu
Loop
Procedure Loeschen
  Deftext 1,0,0,13
  Print At(5,20);Spc(230)
Return
