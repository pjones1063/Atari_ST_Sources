Procedure Laden
  Fileselect "DATOBERT\*.DTO*",Bn$,An$
  If An$=""
    Do
      On Menu
    Loop
  Endif
  Defmouse 2
  Open "I",#1,An$
  Input #1,Ueb1$,Ueb2$,Rei,Spa
  For I=1 To Rei
    Input #1,N$(I)
    For J=1 To Spa
      Input #1,W(I,J)
    Next J
  Next I
  For I=1 To Spa
    Input #1,Nsa$(I),Nsb$(I)
  Next I
  Input #1,Maks
  Close #1
  Defmouse 3
  Mnx=3
  Gosub Mnakt
  Do
    On Menu
  Loop
  Stop
Return
' ######################         #########
Procedure Speichern
  Fileselect "DATOBERT\*.DTO",Bn$,An$
  If An$=""
    Do
      On Menu
    Loop
  Endif
  Defmouse 2
  Open "O",#1,An$+".GRF"
  Write #1,Ueb1$,Ueb2$,Rei,Spa
  For I=1 To Rei
    Write #1,N$(I)
    For J=1 To Spa
      Write #1,W(I,J)
    Next J
  Next I
  For I=1 To Spa
    Write #1,Nsa$(I),Nsb$(I)
  Next I
  Write #1,Maks
  Close #1
  Defmouse 3
  Do
    On Menu
  Loop
Return
