Procedure Hardcpa4
  Hidem
  Local N%,M%,L%,K%,J%,I%,B$
  Lprint Chr$(27);"A";Chr$(8);Chr$(27);"2";
  Quench%=Max(Quench%,300)
  L%=800-400 Div Quench%
  M%=L% Div 256
  N%=L%-M%*256
  L%=Xbios(2)
  For K%=L% To L%+79
    Lprint " ";
    Lprint Chr$(27);"L";Chr$(N%);Chr$(M%);
    J%=0
    For I%=K%+31920 To K% Step -80
      B$=Chr$(Peek(I%))
      Inc J%
      If J%<Quench% Then
        Lprint B$+B$;
      Else
        Lprint B$;
        J%=0
      Endif
    Next I%
    Lprint
  Next K%
  Lprint Chr$(27);"A";Chr$(12);Chr$(27);"2";
  Showm
Return
