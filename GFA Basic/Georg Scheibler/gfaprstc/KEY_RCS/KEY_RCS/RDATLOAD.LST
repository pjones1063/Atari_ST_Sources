' DATA - Lader fÅr KEY RESOURCE EDITOR
'
' - erzeugt Resource-File KEY_RCS.RSC
' (c) MAXON Computer 1989
'
DIM check%(10),sum%(10)
RESTORE
FOR z&=0 TO 10
  READ x$
  check%(z&)=VAL("&H"+x$)
NEXT z&
CLR z&
DO
  READ x$
  EXIT IF x$="*"
  v%=VAL("&H"+x$)
  sum%(SHR&(z&,6))=(sum%(SHR&(z&,6))+v%*z&) AND &HFFFFFF
  INC z&
LOOP
'
fehler!=FALSE
FOR z&=0 TO 10
  IF sum%(z&)<>check%(z&)
    PRINT "FEHLER IN BLOCK ";z&
    fehler!=TRUE
  ENDIF
NEXT z&
IF NOT fehler!
  RESTORE rsc_datas
  OPEN "O",#1,"KEY_RCS.RSC"
  DO
    READ x$
    EXIT IF x$="*"
    PRINT #1,MKI$(VAL("&H"+x$));
  LOOP
  CLOSE #1
  PRINT "RSC-Datei wurde erzeugt!"
ENDIF
'
