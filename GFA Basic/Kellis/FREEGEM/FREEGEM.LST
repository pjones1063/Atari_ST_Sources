> PROCEDURE redraw_text(object&,value%)
LOCAL lenght&
IF object&<>pcentage&
lenght&=8-LEN(STR$(value%))
IF lenght&<=0
lenght&=0
ENDIF
CHAR{{OB_SPEC(tree%,object&)}}=STR$(value%)+SPACE$(lenght&)
ELSE                           ! for pcentages
IF value%<9
p$=CHR$(32)+STR$(value%)+CHR$(32)+CHR$(32)+"%"
ELSE IF value%<100 AND value%>9
p$=CHR$(32)+STR$(value%)+CHR$(32)+"%"
ELSE IF value%>=100
p$="Error"
ENDIF
CHAR{{OB_SPEC(tree%,object&)}}=p$
ENDIF
@win_redraw_objc(handle&,tree%,object&)
RETURN
> FUNCTION exist(p$) !v0.02
$F%
LOCAL r!,e&,sdta%
INLINE dta%,64
p$=p$+CHR$(FALSE)
sdta%=GEMDOS(47) !get old dta
~GEMDOS(26,L:dta%) !set new dta
e&=GEMDOS(78,L:V:p$,W:FALSE)
~GEMDOS(26,L:sdta%) !reset old dta
IF e&=FALSE
r!=TRUE
ELSE
r!=FALSE
ENDIF
RETURN r!
ENDFUNC
