> PROCEDURE init                      ! startup code, initialize everything
  version$="1.8"
  first_open|=1
  obj&=0
  exit=0
  @rsc_define
  @include
  ~WIND_GET(desk&,wf_workxywh&,dsk_x&,dsk_y&,dsk_w&,dsk_h&)            ! get the coordinates   d for desktop
  ~WIND_CALC(0,close&,dsk_x&,dsk_y&,dsk_w&,dsk_h&,newx&,newy&,neww&,newh&)
  hwidget&=newh&-dsk_h&
  ' ~FORM_DIAL(3,0,0,0,0,dsk_x&,dsk_y&,dsk_w&,dsk_h&)    ! this one cls the screen when NOT compiled
  ~GRAF_MOUSE(arrow&,0)                                       ! reset mouse pointer
  IF NOT compiled!
    program_path$="g:\minta\freegem\"
  ELSE
    program_path$=CHR$(65+GEMDOS(25))+":"+DIR$(0)+"\"      ! get dynamical path ONCE compiled
  ENDIF
  success|=RSRC_LOAD(program_path$+"freegem.rsc")
  IF success|<>1                                         ! rsc loaded correctly?
    ALERT 1,"File freegem.rsc not found. ",1,"OK",ok|
    EDIT
  ENDIF
  ~RSRC_GADDR(r_tree&,0,tree%)
  ~RSRC_GADDR(r_string&,alert1&,alert1%)
  ~RSRC_GADDR(r_string&,alert2&,alert2%)
  ~RSRC_GADDR(r_string&,alert3&,alert3%)
  xres&=WORK_OUT(0)
  yres&=WORK_OUT(1)
  tx&=OB_X(tree%,0)
  ty&=OB_Y(tree%,0)
  tw&=OB_W(tree%,0)
  th&=OB_H(tree%,0)
  abs_width&=OB_W(tree%,16)
  title$="Free GEM V"+version$+CHR$(0)                          ! title of the window
  w%=V:title$
  magx&=@cookie_find("MagX")
  handle1&=WIND_CREATE(name&+move&+close&+fuller&,0,0,dsk_w&,dsk_h&)         ! creation of the window for our rsc &X0 because NO GEM widgets
  handle2&=WIND_CREATE(name&+move&+close&,0,0,dsk_w&,dsk_h&)         ! creation of the window for our rsc &X0 because NO GEM widgets
  handle3&=WIND_CREATE(0,0,0,dsk_w&,dsk_h&)         ! creation of the window for our rsc &X0 because NO GEM widgets
  IF magx&=-1 AND VAL(@aes$)>=410             ! cookie not found
    handle&=handle1&
  ELSE
    handle&=handle2&
  ENDIF
  IF handle&<1 OR handle3&<1
    ~FORM_ALERT(1,CHAR{alert2%})
    exit=1
  ENDIF
  IF LPEEK(&H5A8)=&H1357BD13
    full_ttram%=LPEEK(&H5A4)-(16384*1024)
  ENDIF
  IF full_ttram%<=0
    full_ttram%=0
    ~OBJC_DELETE(tree%,ttr&)
  ENDIF
  fullram%=LPEEK(&H42E)+full_ttram%
  ~WIND_SET(handle&,2,CARD(SWAP(w%)),CARD(w%),0,0)
RETURN
