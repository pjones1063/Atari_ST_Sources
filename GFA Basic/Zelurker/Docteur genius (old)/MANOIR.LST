PROCEDURE 6000
  REM    ANALYSE DU MOT
  n=0
  gn=0
  REPEAT
    n=n+1
  UNTIL MID$(x$,n,1)<>" "
  REPEAT
    gn=gn+1
  UNTIL MID$(x$,n+gn,1)=" " OR MID$(x$,n+gn,1)="" OR gn=20
  IF gn>4 THEN
    gn=4
  ENDIF
  x$(1)=MID$(x$,n,gn)
  REPEAT
    n=n+1
  UNTIL MID$(x$,n,1)=" " OR MID$(x$,n,1)=""
  REPEAT
    n=n+1
  UNTIL MID$(x$,n,1)<>" "
  gn=0
  REPEAT
    gn=gn+1
  UNTIL MID$(x$,n+gn,1)=" " OR MID$(x$,n+gn,1)="" OR gn=20
  IF gn>4 THEN
    gn=4
  ENDIF
  x$(2)=MID$(x$,n,gn)
  FOR w=1 TO 2
    n=0
    mo$(w)="00"
  6170:
    n=n+1
    IF n>v THEN
      w=3
      GOTO 6300
    ENDIF
    IF MID$(v$(n),3,4)=x$(w) THEN
      GOTO 6250
    ENDIF
    GOTO 6170
  6250:
    mo$(w)=MID$(v$(n),1,2)
  6300:
  NEXT w
RETURN
