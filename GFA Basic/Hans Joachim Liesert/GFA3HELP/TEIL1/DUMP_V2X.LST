'  V2.xx-QUASI-DUMP:
'
DIM b.len%(12),vtp$(12)
'   DIM b.len%(16),vtp$(16)   ! fÅr V3.0
FILESELECT "*.BAS","",s$
'   FILESELECT "*.GFA","",s$  ! fÅr V3.0
IF EXIST(s$)
OPEN "I",#1,s$
SEEK #1,12
FOR i%=0 TO 11
' FOR i%=0 TO 15 ! fÅr V3.0
READ vtp$(i%)
BGET #1,VARPTR(b.len%(i%)),4
NEXT i%
FOR i%=1 TO 12
' FOR i%=1 TO 16  ! fÅr V3.0
IF b.len%(i%)>b.len%(i%-1)
SEEK #1,126+b.len%(i%-1)
' SEEK #1,164+b.len%(i%-1)  ! fÅr V3.0
titel$=LEFT$(vtp$(i%-1),LEN(vtp$(i%-1))-10)
vblk$=vblk$+"' "+CHR$(13)+"' "+titel$+CHR$(13)
vblk$=vblk$+"' "+STRING$(30,"=")+CHR$(13)
FOR j%=1 TO b.len%(i%)-b.len%(i%-1)
byte%=INP(#1)
ADD j%,byte%
buff$=SPACE$(byte%)
BGET #1,VARPTR(buff$),byte%
EXIT IF LEN(vblk$)+LEN(RIGHT$(vtp$(i%-1),10)+CHR$(13))>32767
IF buff$>""
IF LEFT$(titel$,3)<>"Pro"
' fÅr V3.0
' vblk$=vblk$+"'   "+buff$+TRIM$(RIGHT$(vtp$(i%-1),10))+CHR$(13)
vblk$=vblk$+"'   "+buff$+RIGHT$(vtp$(i%-1),10)+CHR$(13)
ELSE
vblk$=vblk$+"'   "+RIGHT$(vtp$(i%-1),9)+" "+buff$+CHR$(13)
ENDIF
ENDIF
NEXT j%
ENDIF
EXIT IF LEN(vblk$)+LEN(RIGHT$(vtp$(i%-1),10)+CHR$(13))>32767
NEXT i%
CLOSE
OPEN "O",#1,"VARS.LST"
PRINT #1;vblk$
CLOSE
ENDIF
