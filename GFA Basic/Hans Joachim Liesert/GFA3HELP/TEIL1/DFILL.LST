GRAPHMODE 2
FOR i%=0 TO 7
DEFFILL i%,3,i%
PCIRCLE 16,16,16
DEFFILL i%+2,3,i%+2
PCIRCLE 48,16,16
NEXT i%
@dfill3(7,7,df$)    ! fÅr V3.0
' @Dfill2(7,7,*Df$) ! fÅr V2.xx
DEFFILL 1,df$
PBOX 15,64,192,192
GRAPHMODE 3
@dfill3(42,7,df$)    ! fÅr V3.0
' @Dfill2(42,7,*Df$) ! fÅr V2.xx
DEFFILL 1,df$
PBOX 105,14,192,112
'
PROCEDURE dfill3(d.x%,d.y%,VAR d.ff$)
LOCAL d.fr$,dc1%,dc2%,d.f$,xb%
GET d.x%,d.y%,d.x%+15,d.y%+15,d.fr$
xb%=XBIOS(4)
IF xb%=2
d.f$=RIGHT$(d.fr$,32)
ELSE
FOR dc1%=0 TO 3-xb%*2
FOR dc2%=0 TO 15
d.f$=d.f$+MID$(d.fr$,7+dc1%*2+dc2%*(8-xb%*4),2)
NEXT dc2%
NEXT dc1%
ENDIF
d.ff$=d.f$
RETURN
PROCEDURE dfill2(d.x%,d.y%,d.f%)
LOCAL d.fr$
GET d.x%,d.y%,d.x%+15,d.y%+15,d.fr$
*d.f%=RIGHT$(d.fr$,32)
RETURN
