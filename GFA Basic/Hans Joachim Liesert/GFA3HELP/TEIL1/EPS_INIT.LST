' EPS_EDIT zu LPRINT:
'
DIM d.str$(7)
ascii%=35
d.str$(0)="00000000000"
d.str$(1)="00110001100"
d.str$(2)="00110001100"
d.str$(3)="01111111110"
d.str$(4)="00110001100"
d.str$(5)="01111111110"
d.str$(6)="00110001100"
d.str$(7)="00110001100"
epdat$="D "+STR$(ascii%)+","
FOR epi%=1 TO 11
CLR sp%
FOR epj%=0 TO 7
ADD sp%,(2^(7-epj%)*SGN(VAL("&X"+d.str$(epj%)) AND 2^(11-epi%)))
NEXT epj%
epdat$=epdat$+STR$(sp%)+","
NEXT epi%
epdat$=epdat$+CHR$(34)+SPACE$(50-LEN(epdat$))+"Chr:"
epdat$=epdat$+CHR$(ascii%)+" / Asc:"+STR$(ascii%)+CHR$(34)
OPEN "O",#99,"EPS_DATA.LST"
PRINT #99,epdat$;CHR$(13)
CLOSE #99
'
'
'
@eps_init
PROCEDURE eps_init
