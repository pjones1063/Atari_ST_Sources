' SETCOLOR - Beispiel (nur fÅr LOWRES):
'
DIM c%(15)
SETCOLOR 0,0
FOR i%=1 TO 15
  IF i%<8
    c%(i%)=i%*16
  ELSE
    c%(i%)=7*256+(15-i%)*16
  ENDIF
  SETCOLOR i%,c%(i%)
NEXT i%
FOR j%=0 TO 4
  RESTORE
  FOR i%=1 TO 15
    READ a%
    COLOR a%
    BOX 60+j%*20+i%,j%*20+i%,260-j%*20-i%,199-j%*20-i%
    CIRCLE 160,100,j%*15+i%*10
    CIRCLE 160,100,j%*15+i%*10+5
  NEXT i%
NEXT j%
DO
  b%=c%(15)
  FOR i%=14 DOWNTO 1
    c%(i%+1)=c%(i%)
  NEXT i%
  c%(1)=b%
  FOR i%=1 TO 15
    FOR j%=1 TO 100
    NEXT j%
    SETCOLOR i%,c%(i%)
  NEXT i%
LOOP
DATA 2,3,6,4,7,5,8,9,10,11,14,12,15,13,1, Umrechnungstabelle
