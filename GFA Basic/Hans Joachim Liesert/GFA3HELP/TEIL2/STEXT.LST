PROCEDURE stext(s.xt%,s.yt%,s.xl%,s.txt$,s.xo%,s.yo%)
  GRAPHMODE 2
  TEXT s.xt%-1,s.yt%,s.xl%,s.txt$
  TEXT s.xt%+1,s.yt%,s.xl%,s.txt$
  TEXT s.xt%,s.yt%-1,s.xl%,s.txt$
  TEXT s.xt%,s.yt%+1,s.xl%,s.txt$
  TEXT s.xt%+s.xo%,s.yt%+s.yo%,s.xl%,s.txt$
  GRAPHMODE 3
  TEXT s.xt%,s.yt%,s.xl%,s.txt$
  GRAPHMODE 1
RETURN
