'
' -----------------------
' - get the player name -
' -----------------------
'
PROCEDURE end_of_game
'
  fade_to_black(0,0,0)
  CLS
  @set_blue_palette
'
  PUT 131, 20, title$
  @draw_text("---------",-1,13)
  @draw_text("game over",-1,14)
  @draw_text("---------",-1,15)
  fade_to_color(0,0,0)
'  
  nbl=0
  place=-1
'
  FOR i=0 TO 9
     IF score% >= scores(i)
       place=i
     ENDIF
     EXIT IF place>=0
  NEXT i
'
  IF place >= 0
    FOR j=9 TO place+1 STEP -1
      scores(j) = scores(j-1)
      snames$(j) = snames$(j-1)
    NEXT j
'
' clears keyboard buffer
'
    DO
    LOOP UNTIL INKEY$=""
'
    nam$=">>> ... <<<"
    @draw_text( "enter your name",-1,18)
    @draw_text( nam$,-1,20)
    DO
      d$=INPUT$(1)
      a%=ASC(d$)
      IF a%>0
        IF a%==13 OR a%=27 
          nbl=3
        ELSE IF a%==8 AND nbl>0
          MID$(nam$,nbl+4)="."
          DEC nbl
        ELSE IF d$<>","
          INC nbl
          MID$(nam$,nbl+4)=d$
        ENDIF
        @draw_text( nam$,-1,20)
      ENDIF
      EXIT IF nbl=3
    LOOP
'
    snames$(place) = MID$(nam$,5,3)
    scores(place) = score%
    @save_scores
  ENDIF
'
  score%=0
  PAUSE 25
'
RETURN
