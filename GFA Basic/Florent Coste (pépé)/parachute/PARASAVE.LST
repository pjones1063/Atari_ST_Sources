> PROCEDURE halloffame
'
  @fade_to_black(2,3,4)
  CLS
'  
  @draw_text("------------",-1,3)
  @draw_text("HALL OF FAME",-1,4)
  @draw_text("------------",-1,5)
'
  FOR i=0 TO 9
    @draw_text( snames$(i), 14,9+i )
