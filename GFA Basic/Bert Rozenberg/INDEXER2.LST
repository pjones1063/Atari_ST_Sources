Procedure Verw_disk
  Do
    Clearw 1
    Box 170,85*Kleur,470,100*Kleur
    Print At(25,12*Kleur);"Geef Extensie:  ";
    Form Input (3),Ext$
    Exit If Len(Ext$)<>0
  Loop
Return
