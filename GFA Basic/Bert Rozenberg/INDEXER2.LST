Procedure Suche(Path2$,File$,Out$)
  Oldpath$=Dir$(0)
  Olddrv%=Gemdos(25)+1
  '  Open "O",#1,Out$
  If Instr(Path2$,":")
    Chdrive Asc(Path2$)-64
    Path2$=Mid$(Path2$,Instr(Path2$,":")+1)
  Endif
  Chdir Path2$
  Void Gemdos(26,L:Basepage+128)           !setdta
  File$=File$+Chr$(0)
  Star$="*.*"+Chr$(0)
  Drv$=Chr$(Gemdos(25)+65)+":"
  @Suche1
  '  Close #1
  Chdir "\"+Olddir$
  Chdrive Olddrv%
Return
Procedure Suche1
  Local W%
  @Fsfirst
  While E%=0
    Path$=Dir$(0)
    Gosub Get_respond
    If Respond%=1
      Inc N%
      Regel$(N%)=Naam$+Ext$+"  "+Grootte$+"  "+Right$(Datum$,2)+Mid$(Datum$,4,2)+Left$(Datum$,2)+"  "+Disknaam$+"  "+Path$
    Endif
    '    Print #1,Z$'''Drv$+Dir$(0)+"\"
    @Fsnext
  Wend
  @Fsfirstdir
  Q%=0
  While E%=0
    If T% And 16
      If X$<>"." And X$<>".."
        W%=Q%
        Chdir X$
        @Suche1
        Chdir ".."
        @Fsfirstdir
        Q%=0
        While W%<>Q%
          Void Gemdos(79)
          Inc Q%
        Wend
      Endif
    Endif
    @Fsnext
    Inc Q%
  Wend
Return
Procedure Fsfirst
  E%=Gemdos(78,L:Varptr(File$),&H27)    !fsfirst
  @Getnam
Return
Procedure Fsfirstdir
  E%=Gemdos(78,L:Varptr(Star$),16)
  @Getnam
Return
Procedure Fsnext
  E%=Gemdos(79)
  @Getnam
Return
Procedure Getnam
  If E%
    X$=""
    T%=0
  Else
    X$=Space$(20)
    Bmove Basepage+158,Varptr(X$),14
    X$=Left$(X$,Instr(X$,Chr$(0))-1)
    T%=Peek(Basepage+149)                 !dta+21 Attribute
    '
    '
    '
    '
    '
    '
    Z$=Space$(20)
    Bmove Basepage+158,Varptr(Z$),14
    ' Filename
    Z$=Left$(Z$,Instr(Z$,Chr$(0))-1)
    A=Instr(Z$,".")
    If A>1
      Naam$=Left$(Left$(Z$,A-1)+Space$(10),9)
      Ext$=Mid$(Z$,A,4)
    Else
      Naam$=Left$(Z$+Space$(10),9)
      Ext$="    "
    Endif
    ' AuffÅllen auf gleiche LÑnge
    '    Z$=Left$(Z$+Space$(20),15)
    ' FilelÑnge oder <<DIR>>
    L$=Space$(9)
    A%=Peek(Basepage+149)                 !dta+21 Attribute
    If A% And 16
      Rset L$="<<DIR>>"
    Else
      If A% And 8
        Rset L$="<LABEL>"         !Diskettenname
      Else
        Rset L$=Str$(Lpeek(Basepage+154))  !dta+26=filelÑnge
      Endif
    Endif
    Grootte$=L$
    '    Z$=Z$+L$
    '
    ' Datum               dpeek(dta+24)
    '
    D%=Dpeek(Basepage+152)
    D$=Right$("0"+Str$(D% And 31),2)+"."
    D$=D$+Right$("0"+Str$(D%/32 And 15),2)+"."
    D$=D$+Str$(D% Div 512+80)
    Datum$=D$
  Endif
Return
