Procedure Printl(Pr$)
  If Tab%=58
    Tab%=0
    Lprint Chr$(12)
  Endif
  If Tab%=0
    Lprint
    Lprint
    Lprint Regel$(0)
    Lprint
  Endif
  Lprint Pr$
  Print At(1,7*Kleur);Space$(80)
  Print At(1,7*Kleur);Pr$
  Inc Tab%
Return
'
'
'
Procedure Prints(Pr$)
  If Tab%=3*Kleur
    Print At(32,21*Kleur);"KLIK MUIS_TOETS"
    Gosub Get_mouse
    Sound 2,13,3,6,10
    Sound 2,0
    Clearw 1
    Print At(2,Kleur);Regel$(0)
  Endif
  Print At(2,Tab%);Pr$
  Inc Tab%
  If Tab%=20*Kleur
    Tab%=3*Kleur
  Endif
Return
