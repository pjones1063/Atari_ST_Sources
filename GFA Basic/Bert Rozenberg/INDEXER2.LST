    Endif
    For K%=1 To N%
      P=0
      If A$="*"
        If Instr(Mid$(Regel$(K%),Soort(1),4),A2$)<>0
          '  If Instr(Left$(Regel$(K%),10),A2$)<>0
          P=1
        Endif
      Else
        If Mid$(Regel$(K%),11,Len(Ext$))=Ext$
          P=1
