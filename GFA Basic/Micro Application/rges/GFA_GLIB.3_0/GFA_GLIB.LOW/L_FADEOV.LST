PROCEDURE gfa_l_fadeov__(destadr__%,sourceadr__%,rows__%,speed__%,options__%,pattern__%)
  LOCAL codeadr__%
  INLINE codeadr__%,1870
  IF BYTE{codeadr__%}=&H48
    ~C:codeadr__%(L:destadr__%,L:sourceadr__%,W:rows__%,W:speed__%,W:options__%,W:pattern__%)
    lib_rv__%=0
  ELSE
    lib_rv__%=-1
  ENDIF
RETURN
