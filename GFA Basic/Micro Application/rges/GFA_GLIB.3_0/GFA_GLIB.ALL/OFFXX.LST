PROCEDURE gfa_off14__(x1__%,y1__%,x2__%,y2__%,speed__%,col__%)
  LOCAL i1__%,i2__%,mavx__%,mavy__%
  IF x1__%>x2__%
    SWAP x1__%,x2__%
  ENDIF
  IF y1__%>y2__%
    SWAP y1__%,y2__%
  ENDIF
  mavx__%=(x2__%-x1__%)/2
  mavy__%=(y2__%-y1__%)/2
  COLOR col__%
  i1__%=0
  FOR i2__%=0 TO mavy__%
    VSYNC
    BOX x1__%+i1__%,y1__%+i2__%,x2__%-i1__%,y2__%-i2__%
    IF i1__%<mavx__%
      INC i1__%
    ENDIF
    PAUSE speed__%
  NEXT i2__%
RETURN
