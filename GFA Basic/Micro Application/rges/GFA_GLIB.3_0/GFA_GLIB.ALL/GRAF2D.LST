PROCEDURE gfa_graf2d__(x__%,y__%,char__$,options__%,miroir__%,angle__%,xfacteur__,yfacteur__,xpenche__%,flag__!)
  LOCAL b__%,hauteur__%,quantite__%,aide__%
  IF graf2din__!=FALSE
    lib_rv__%=0
    DIM graf2dal__%(5000)
    DIM graf2dko__%(200)
    DIM graf2dpo__%(40)
    quantite__%=0
    RESTORE pos2d__
    DO
      READ b__%
      EXIT IF b__%=-1
      graf2dpo__%(quantite__%)=b__%
      INC quantite__%
    LOOP
    quantite__%=0
    RESTORE graf2dhx__
    DO
      READ b__%
      EXIT IF b__%=-999
      graf2dal__%(quantite__%)=b__%
      INC quantite__%
    LOOP
    LET graf2din__!=TRUE
  ELSE
    hauteur__%=50
    b__%=ASC(char__$)
    IF b__%>64 AND b__%<91
      lib_rv__%=0
      SUB b__%,65
      aide__%=graf2dpo__%(b__%)
      quantite__%=graf2dpo__%(b__%+1)-aide__%
      FOR b__%=0 TO quantite__%
        graf2dko__%(b__%)=graf2dal__%(b__%+aide__%)
      NEXT b__%
      xoffset__%=x__%
      yoffset__%=y__%
      IF (options__% AND 1)
        ' -------------------------- ‚chelle
        FOR i__%=0 TO quantite__% STEP 2
          graf2dko__%(i__%)=graf2dko__%(i__%)*xfacteur__
          graf2dko__%(i__%+1)=graf2dko__%(i__%+1)*yfacteur__
        NEXT i__%
      ENDIF
      IF (options__% AND 2)
        ' --------------------------- rotation
        b__=angle__%*PI/180
        var1__=SIN(b__)
        var2__=COS(b__)
        FOR i__%=0 TO quantite__% STEP 2
          xs__%=graf2dko__%(i__%)
          ys__%=-graf2dko__%(i__%+1)
          graf2dko__%(i__%)=xs__%*var2__+ys__%*var1__
          graf2dko__%(i__%+1)=-(ys__%*var2__-xs__%*var1__)
        NEXT i__%
      ENDIF
      IF (options__% AND 4)
        ' -------------------------- ‚longation
        FOR i__%=0 TO quantite__% STEP 2
          graf2dko__%(i__%)=graf2dko__%(i__%)+(graf2dko__%(i__%+1)*xpenche__%/hauteur__%)
        NEXT i__%
      ENDIF
      IF (options__% AND 8)
        ' -------------------------- miroir
        IF miroir__%=1
          FOR i__%=0 TO quantite__% STEP 2
            graf2dko__%(i__%)=-graf2dko__%(i__%)
          NEXT i__%
        ELSE
          FOR i__%=1 TO quantite__% STEP 2
            graf2dko__%(i__%)=-graf2dko__%(i__%)
          NEXT i__%
        ENDIF
      ENDIF
      SUB quantite__%,2
      '  Show
      FOR i__%=0 TO quantite__%-1 STEP 4
        LINE graf2dko__%(i__%)+xoffset__%,graf2dko__%(i__%+1)+yoffset__%,graf2dko__%(i__%+2)+xoffset__%,graf2dko__%(i__%+3)+yoffset__%
      NEXT i__%
      IF flag__!=TRUE
        FILL graf2dko__%(quantite__%)+xoffset__%,graf2dko__%(quantite__%+1)+yoffset__%
      ENDIF
    ELSE
      lib_rv__%=1
    ENDIF
  ENDIF
