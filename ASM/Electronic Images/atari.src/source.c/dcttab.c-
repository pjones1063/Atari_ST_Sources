short	table[65536];

void init_dct_tab(void)
{	int x;
	x = 0;
	short *ptr= table;
	do {	int z = (unsigned short) x; 
		int y = z - 64;
		if (y<0) y = 0;
		if (y>127) y = 127;
		z = x >> 8;
		z += 0xffc0;
		if (z<0> z = 0;
		if (z>127) z = 127;
		z <<= 8;
		z Ý= y;
		*ptr++ = z;
		x++;
	} while ( x <= 65535);
}
